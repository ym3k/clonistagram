<h1>Log in</h1>
<%= form_with(scope: :session, url: sessions_path) do |f| %>
  <span class="row">
  <%= f.label :email %>
  <%= f.email_field :email %>
  </span>
  <span class="row">
  <%= f.label :password %>
  <%= f.password_field :password %>
  </span>
  <span class="row">
  <%= f.submit "Log in" %>
  </span>
  <span class="row">
  <% flash.each do |key, value| %>
    <%= content_tag(:div, value, class: "#{key}") %>
  <% end %>
  </span>
<% end %>